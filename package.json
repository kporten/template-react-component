{
  "name": "template-react-component",
  "version": "3.2.0",
  "private": false,
  "license": "MIT",
  "description": "Template to develop a React component",
  "keywords": [
    "react",
    "template",
    "typescript",
    "component"
  ],
  "repository": "https://github.com/kporten/template-react-component.git",
  "bugs": "https://github.com/kporten/template-react-component/issues",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "scripts": {
    "start": "start-storybook --port 9009",
    "start:ci": "start-storybook --ci --quiet --port 9009",
    "test": "jest --watch --notify",
    "test:all": "jest --watchAll --coverage --notify",
    "test:ci": "jest --ci --runInBand --coverage",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:ci": "start-server-and-test start:ci 9009 e2e:run",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "type-check": "tsc --project ./tsconfig.json && tsc --project ./cypress/tsconfig.json",
    "changelog": "auto-changelog --package --template keepachangelog --sort-commits relevance",
    "build": "rollup -c",
    "build:storybook": "build-storybook",
    "commit": "git-cz",
    "version": "yarn build && yarn changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags && yarn publish --non-interactive"
  },
  "main": "./dist/lib/index.cjs.js",
  "module": "./dist/lib/index.es.js",
  "files": [
    "dist/lib"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-url": "^6.0.0",
    "@storybook/addon-a11y": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.2",
    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "auto-changelog": "^2.2.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.3",
    "cypress": "^6.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.38.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-subpath-externals": "^3.4.0",
    "start-server-and-test": "^1.11.7",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1"
  }
}
